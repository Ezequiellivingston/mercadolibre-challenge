{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "prettier.enable": false,
  "css.validate": false,
  "stylelint.enable": true,
  "stylelint.validate": [
    "css",
    "scss"
  ],
}